<div class="flex flex-wrap w-1/3">
        <div class="w-full p-1 md:p-2">
                {{#if images}}
                    {{#each images as |image|}}
                        {{> image-display}}
                    {{/each}}
                {{/if}}
        </div>
        <div class="pt-2 pl-2">
                <div class="pb-2">{{name}}</div>
                <div>{{description}}</div>
        </div>
</div>